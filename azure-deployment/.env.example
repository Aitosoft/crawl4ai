# Azure Deployment Environment Variables
# Copy this file to .env and add your actual values
# .env is gitignored and will NOT be committed

# LLM Provider API Keys (required for extraction)
OPENAI_API_KEY=sk-your-openai-key-here
DEEPSEEK_API_KEY=your-deepseek-key-here
GEMINI_API_TOKEN=your-gemini-token-here

# Azure Container Apps Configuration
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP=crawl4ai-v2-rg
AZURE_LOCATION=northeurope

# Key Vault Configuration (if using)
KEY_VAULT_NAME=crawl4ai-v2-keyvault
API_TOKEN_SECRET_NAME=crawl4ai-api-token

# Application Configuration
CRAWL4AI_IMAGE=unclecode/crawl4ai:0.7.7
